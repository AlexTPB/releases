local re = a:NewTab("Credits")
local f = re:NewSection("Made by tpb.mp4#6086")
f:NewLabel("UI Library: Kavo")
f:NewKeybind("Toggle Emote UI", "This is the keybind for toggling UI", Enum.KeyCode.Comma, function()
	Library:ToggleUI()
end)
