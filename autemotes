-- unobfuscated cause its really simple to make just do an emote and copy the remote for it and just edit the emote name argument

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local a = Library.CreateLib("tpb's AUT Emotes Pack GUI", "LightTheme")
local e = a:NewTab("Emotes")
local b = e:NewSection("AUT Emotes Pack:")

b:NewButton("Smooth Moves","Plays The Emote",function()
    local args = {[1] = "SmoothMoves"}
    game:GetService("ReplicatedStorage"):WaitForChild("ReplicatedModules"):WaitForChild("KnitPackage"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("EmoteService"):WaitForChild("RF"):WaitForChild("Play"):InvokeServer(unpack(args))
    Library:ToggleUI()
end)
b:NewButton("Smooth Criminal","Plays The Emote",function()
    local args = {[1] = "SmoothCriminal"}
    game:GetService("ReplicatedStorage"):WaitForChild("ReplicatedModules"):WaitForChild("KnitPackage"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("EmoteService"):WaitForChild("RF"):WaitForChild("Play"):InvokeServer(unpack(args))
    Library:ToggleUI()
end)
